<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="sharedpref" path="auto_backup_prefs.xml"/>
        <exclude domain="database" path="backup.db"/>
        <exclude domain="file" path="backup_files/"/>
        <exclude domain="external" path="Android/data/com.personal.autobackup"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="auto_backup_prefs.xml"/>
        <exclude domain="database" path="backup.db"/>
        <exclude domain="file" path="backup_files/"/>
        <exclude domain="external" path="Android/data/com.personal.autobackup"/>
    </device-transfer>
</data-extraction-rules>
